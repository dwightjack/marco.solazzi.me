{
    "presets": [
        "react",
        ["es2015", {"loose": true}]
    ],
    "plugins" : [
        "transform-object-rest-spread",
        ["transform-runtime", {"polyfill": false, "regenerator": false }]
    ],

    "env": {
        "production": {
            "plugins": [
                ["transform-runtime", {"polyfill": false, "regenerator": false }],
                "transform-react-remove-prop-types",
                "transform-react-constant-elements"
            ]
        }
    }
}
