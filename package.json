    "eslint-plugin-import": "1.14.0",
    "eslint-plugin-jsx-a11y": "2.2.0",
    "eslint-plugin-react": "6.1.2",
    "eventsource-polyfill": "0.9.6",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "glob": "7.0.6",
    "html-webpack-plugin": "2.22.0",
    "image-size": "0.5.0",
