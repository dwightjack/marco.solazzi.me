{
  "name": "resume",
  "description": "My Resume",
  "version": "0.0.1",
  "license": "MIT",
  "author": {
    "name": "Marco Solazzi"
  },
  "contributors": [],
  "main": "frontend/assets/javascripts/application.js",
  "engines": {
    "node": "^4.0.0"
  },
  "scripts": {
    "start": "npm run clean && npm run modernizr && cross-env NODE_ENV=development node ./build/scripts/server.js",
    "server": "cross-env NODE_ENV=production node ./build/scripts/server.js",
    "webpack": "cross-env NODE_ENV=development webpack --config build/config/webpack.dev.js --color",
    "webpack:production": "cross-env NODE_ENV=production webpack --config build/config/webpack.prod.js --color -p",
    "modernizr": "cross-env NODE_ENV=development node ./build/scripts/modernizr",
    "modernizr:production": "cross-env NODE_ENV=production node ./build/scripts/modernizr",
    "clean": "rimraf ./public/*",
    "build": "npm run clean && npm run modernizr && npm run webpack",
    "build:production": "npm run clean && npm run modernizr:production && npm run webpack:production",
    "eslint": "eslint -c ./application/assets/js/.eslintrc \"./application/assets/js/**\"",
    "sasslint": "sass-lint -c .sass-lint.yml 'application/assets/**/*.scss' -v -q -i 'application/assets/vendors/**/*.scss'",
    "lint": "npm run eslint && npm run sasslint"
  },
  "dependencies": {
    "babel-polyfill": "6.16.0",
    "babel-runtime": "6.18.0",
    "classnames": "2.2.5",
    "gsap": "1.19.0",
    "hoist-non-react-statics": "1.2.0",
    "lodash": "4.16.5",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "react-smooth-scrollbar": "7.2.0",
    "redux": "3.6.0",
    "redux-actions": "0.13.0",
    "redux-thunk": "2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "6.5.1",
    "babel-core": "6.18.0",
    "babel-eslint": "7.1.0",
    "babel-loader": "6.2.7",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "0.2.10",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "chalk": "1.1.3",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "customizr": "https://github.com/doctyper/customizr/tarball/develop",
    "eslint": "3.9.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-import-resolver-webpack": "0.6.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "eventsource-polyfill": "0.9.6",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "glob": "7.1.1",
    "html-webpack-plugin": "2.24.1",
    "image-size": "0.5.0",
    "image-webpack-loader": "3.0.0",
    "imports-loader": "0.6.5",
    "ip": "1.1.4",
