{
  "name": "resume",
  "description": "My Resume",
  "version": "0.0.1",
  "license": "MIT",
  "author": {
    "name": "Marco Solazzi"
  },
  "contributors": [],
  "main": "frontend/assets/javascripts/application.js",
  "engines": {
    "node": "^4.0.0"
  },
  "scripts": {
    "start": "npm run clean && npm run modernizr && cross-env NODE_ENV=development node ./build/scripts/server.js",
    "server": "cross-env NODE_ENV=production node ./build/scripts/server.js",
    "webpack": "cross-env NODE_ENV=development webpack --config build/config/webpack.dev.js --color",
    "webpack:production": "cross-env NODE_ENV=production webpack --config build/config/webpack.prod.js --color -p",
    "modernizr": "cross-env NODE_ENV=development node ./build/scripts/modernizr",
    "modernizr:production": "cross-env NODE_ENV=production node ./build/scripts/modernizr",
    "clean": "rimraf ./public/*",
    "build": "npm run clean && npm run modernizr && npm run webpack",
    "build:production": "npm run clean && npm run modernizr:production && npm run webpack:production",
    "eslint": "eslint -c ./application/assets/js/.eslintrc \"./application/assets/js/**\"",
    "sasslint": "sass-lint -c .sass-lint.yml 'application/assets/**/*.scss' -v -q -i 'application/assets/vendors/**/*.scss'",
    "lint": "npm run eslint && npm run sasslint"
  },
  "dependencies": {
    "babel-polyfill": "6.9.1",
    "babel-runtime": "6.9.2",
    "gsap": "1.18.5",
    "lodash": "4.13.1",
    "react": "15.2.0",
    "react-dom": "15.2.0"
  },
  "devDependencies": {
    "autoprefixer": "6.3.7",
    "babel-core": "6.10.4",
    "babel-eslint": "6.1.0",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "0.2.7",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-es2015-loose": "7.0.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "chalk": "1.1.3",
    "cross-env": "1.0.8",
    "css-loader": "0.23.1",
    "customizr": "https://github.com/doctyper/customizr/tarball/develop",
    "eslint": "3.0.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-import": "1.10.2",
    "eslint-plugin-jsx-a11y": "1.5.5",
    "eslint-plugin-react": "5.2.2",
    "eventsource-polyfill": "0.9.6",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "glob": "7.0.5",
    "html-webpack-plugin": "2.22.0",
    "image-size": "0.5.0",
    "image-webpack-loader": "1.8.0",
    "imports-loader": "0.6.5",
    "ip": "1.1.3",
    "json-loader": "0.5.4",
    "mkdirp": "0.5.1",
    "modernizr": "3.3.1",
    "node-sass": "3.8.0",
    "portfinder": "1.0.3",
    "postcss-loader": "0.9.1",
    "raw-loader": "0.5.1",
    "resolve-url-loader": "1.6.0",
    "rimraf": "2.5.3",
    "sass-lint": "1.8.2",
    "sass-loader": "4.0.0",
    "style-loader": "0.13.1",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.1"
  }
}
